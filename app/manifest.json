{
  "name": "__MSG_appName__",
  "version": "0.0.9",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "homepage_url": "https://github.com/mradenovic/granoogle",
  "icons": {
    "16": "images/icons/mover16.png",
    "48": "images/icons/mover48.png",
    "128": "images/icons/mover128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/chromereload.js",
      "scripts/background.js"
    ]
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage"
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": false
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/*mpest~entrywc*",
        "http://*/*mpest~entrywc*"
      ],
      "css": [
        "styles/maps.css"
      ],
      "js": [
        "bower_components/jquery/dist/jquery.min.js",
        "scripts/entry_form.js"
      ]
    },
    {
      "matches": [
        "https://*/*mpcharge~chargeswc*",
        "http://*/*mpcharge~chargeswc*"
      ],
      "css": [
        "styles/gmail.css"
      ],
      "js": [
        "bower_components/jquery/dist/jquery.min.js",
        "scripts/charges.js"
      ]
    },
    {
      "matches": [
        "https://*/*mpest~extstopwc*",
        "http://*/*mpest~extstopwc*"
      ],
      "css": [
        "styles/maps.css"
      ],
      "js": [
        "bower_components/jquery/dist/jquery.min.js",
        "scripts/extra_stop.js"
      ]
    },
    {
      "matches": [
        "file://null"
      ],
      "js": [
        "scripts/entry_form_maps.js",
        "scripts/extra_stop_maps.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "scripts/entry_form_maps.js",
    "scripts/extra_stop_maps.js"
  ]
}
